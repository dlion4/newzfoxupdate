      <script>
        // Check if the user has already seen the notification by looking for a cookie
function hasCookieConsent() {
  return document.cookie.indexOf('cookieConsent=true') !== -1;
}

// Function to set a cookie that indicates the user has accepted the cookies
function setCookieConsent() {
  var expirationDate = new Date();
  expirationDate.setFullYear(expirationDate.getFullYear() + 5); // Cookie valid for 1 year
  document.cookie = 'cookieConsent=true; expires=' + expirationDate.toUTCString() + '; path=/';
}

// Show the cookie notification only if the user hasn't seen it before
if (!hasCookieConsent()) {
  var cookieInfoBar = document.getElementById('cookie-info-bar');
  var acceptCookieButton = document.getElementById('accept-cookie');
  var rejectCookieButton = document.getElementById('reject-cookie');

  if (cookieInfoBar && acceptCookieButton && rejectCookieButton) {
    cookieInfoBar.style.display = 'block';

    acceptCookieButton.addEventListener('click', function () {
      setCookieConsent();
      cookieInfoBar.style.display = 'none';
      // You can add code here to handle cookie acceptance
    });

    rejectCookieButton.addEventListener('click', function () {
      cookieInfoBar.style.display = 'none';
      // You can add code here to handle cookie rejection
    });
  }
}
      


